<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

  <display-name>Archetype Created Web Application</display-name>

  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>


  <resource-ref>
    <description>DB Connection</description>
    <res-ref-name>jdbc</res-ref-name>
    <res-type>javax.sql.DataSource</res-type>
    <res-auth>Container</res-auth>
  </resource-ref>

  <servlet>
    <servlet-name>ServletProcessing</servlet-name>
    <servlet-class>ServletProcessing</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletProcessing</servlet-name>
    <url-pattern>/processing</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>ServletCreateTable</servlet-name>
    <servlet-class>ServletCreateTable</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletCreateTable</servlet-name>
    <url-pattern>/createtable</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>ServletCreateDB</servlet-name>
    <servlet-class>ServletCreateDB</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletCreateDB</servlet-name>
    <url-pattern>/createdb</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>ServletPlusData</servlet-name>
    <servlet-class>ServletPlusData</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletPlusData</servlet-name>
    <url-pattern>/servletplusdata</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>ServletPlusRowData</servlet-name>
    <servlet-class>ServletPlusRowData</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletPlusRowData</servlet-name>
    <url-pattern>/servletplusrowdata</url-pattern>
  </servlet-mapping>


  <servlet>
    <servlet-name>ServletDropTable</servlet-name>
    <servlet-class>ServletDropTable</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletDropTable</servlet-name>
    <url-pattern>/servletdroptable</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>ServletDropRow</servlet-name>
    <servlet-class>ServletDropRow</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletDropRow</servlet-name>
    <url-pattern>/servletdroprow</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>ServletDropColumn</servlet-name>
    <servlet-class>ServletDropColumn</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletDropColumn</servlet-name>
    <url-pattern>/servletdropcolumn</url-pattern>
  </servlet-mapping>

</web-app>
